<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans.xsd">
 
	<bean id="empbean" class="com.javawebtutor.Employee">
	    <property name="employeeId" value="101" />
		<property name="employeeName" value="Bivek Kumar" />
	</bean>
	
	
	<!-- by using p-schema -->
	
	<bean id="empbean2" class="com.javawebtutor.Employee" p:employeeId="102" p:employeeName="Aart Jha">
	</bean>
	
 
</beans>