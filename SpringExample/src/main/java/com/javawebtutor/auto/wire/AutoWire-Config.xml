<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans.xsd">
 
	<bean class="com.javawebtutor.auto.wire.Address" name="address">
	    <property name="street" value="Jaitpur"></property>
	    <property name="city" value="Delhi"></property>
	</bean>
	
	<!--Using Autowiring by xml byName  -->
	<bean id="emp" class="com.javawebtutor.auto.wire.Emp" autowire="byName">
	    
	</bean>
	
</beans>